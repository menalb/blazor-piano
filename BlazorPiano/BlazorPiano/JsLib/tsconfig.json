        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
    },
    "include": [
        "src"
    ]
